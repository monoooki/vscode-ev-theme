{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ev Thunder",
  "type": "light",
  "colors": {
    "foreground": "#1D121E",
    "editor.background": "#DDE3E4",
    "editor.foreground": "#1D121E",
    "editorLineNumber.foreground": "#848280",
    "editorLineNumber.activeForeground": "#E2BC07",
    "editorCursor.foreground": "#E2BC07",
    "editor.selectionBackground": "#C7CDCE",
    "editor.inactiveSelectionBackground": "#EAECE8",
    "editor.selectionHighlightBackground": "#E2BC071A",
    "editor.selectionHighlightBorder": "#E2BC0766",
    "editor.wordHighlightBackground": "#E2BC0726",
    "editor.wordHighlightStrongBackground": "#E2BC0726",
    "editor.findMatchBackground": "#E2BC0726",
    "editor.findMatchBorder": "#E2BC0766",
    "editor.findMatchHighlightBackground": "#E2BC071A",
    "editor.findRangeHighlightBackground": "#E2BC0712",
    "editor.lineHighlightBackground": "#D7D8DA",
    "editorWhitespace.foreground": "#C7CDCE",
    "editorIndentGuide.background1": "#C7CDCE",
    "editorIndentGuide.activeBackground1": "#848280",
    "activityBar.background": "#E2BC07",
    "activityBar.foreground": "#1D121E",
    "activityBarBadge.background": "#826C00",
    "activityBarBadge.foreground": "#DDE3E4",
    "sideBar.background": "#EAECE8",
    "sideBar.foreground": "#1D121E",
    "sideBarSectionHeader.background": "#C7CDCE",
    "sideBarSectionHeader.foreground": "#1D121E",
    "sideBarSectionHeader.border": "#BBBDBD",
    "statusBar.background": "#E2BC07",
    "statusBar.foreground": "#1D121E",
    "statusBar.debuggingBackground": "#FFDB2B",
    "statusBar.debuggingForeground": "#1D121E",
    "statusBar.noFolderBackground": "#DDE3E4",
    "titleBar.activeBackground": "#E2BC07",
    "titleBar.activeForeground": "#1D121E",
    "titleBar.inactiveBackground": "#E2BC07",
    "titleBar.inactiveForeground": "#1D121E",
    "commandCenter.foreground": "#1D121E",
    "commandCenter.activeForeground": "#1D121E",
    "commandCenter.inactiveForeground": "#1D121E",
    "tab.activeBackground": "#DDE3E4",
    "tab.activeForeground": "#1D121E",
    "tab.inactiveBackground": "#EAECE8",
    "tab.inactiveForeground": "#A2A2A2",
    "tab.hoverBackground": "#EAECE8",
    "tab.editor-group-tab-background": "#EAECE8",
    "tab.border": "#BBBDBD",
    "tab.activeBorder": "#00000000",
    "tab.activeBorderTop": "#00000000",
    "tab.unfocusedActiveBorder": "#00000000",
    "tab.unfocusedActiveBorderTop": "#00000000",
    "editorGroupHeader.tabsBorder": "#BBBDBD",
    "terminal.background": "#DDE3E4",
    "terminal.foreground": "#1D121E",
    "terminal.ansiBlack": "#C7CDCE",
    "terminal.ansiRed": "#EE5552",
    "terminal.ansiGreen": "#D6B552",
    "terminal.ansiYellow": "#E2BC07",
    "terminalStickyScroll.background": "#D7D8DA",
    "terminal.ansiBlue": "#848280",
    "terminal.ansiMagenta": "#6A3296",
    "terminal.ansiCyan": "#A2A2A2",
    "terminal.ansiWhite": "#BBBDBD",
    "terminal.ansiBrightBlack": "#A2A2A2",
    "terminal.ansiBrightRed": "#F47774",
    "terminal.ansiBrightGreen": "#FFE37A",
    "terminal.ansiBrightYellow": "#FFDB2B",
    "terminal.ansiBrightBlue": "#C7CDCE",
    "terminal.ansiBrightMagenta": "#8A50B5",
    "terminal.ansiBrightCyan": "#DDE3E4",
    "terminal.ansiBrightWhite": "#EAECE8",
    "badge.background": "#826C00",
    "badge.foreground": "#DDE3E4",
    "button.background": "#E2BC07",
    "button.hoverBackground": "#FFDB2B",
    "button.foreground": "#1D121E",
    "button.secondaryBackground": "#DDE3E4",
    "button.secondaryHoverBackground": "#EAECE8",
    "button.secondaryForeground": "#1D121E",
    "extensionButton.background": "#E2BC07",
    "extensionButton.foreground": "#1D121E",
    "extensionButton.hoverBackground": "#FFDB2B",
    "extensionButton.prominentBackground": "#E2BC07",
    "extensionButton.prominentForeground": "#1D121E",
    "extensionButton.prominentHoverBackground": "#FFDB2B",
    "editorWidget.foreground": "#1D121E",
    "editorWidget.background": "#DDE3E4",
    "input.background": "#DDE3E4",
    "input.foreground": "#1D121E",
    "input.border": "#DDE3E4",
    "input.placeholderForeground": "#A2A2A2",
    "dropdown.background": "#C7CDCE",
    "dropdown.foreground": "#1D121E",
    "dropdown.border": "#BBBDBD",
    "widget.shadow": "#84828066",
    "scrollbar.shadow": "#84828033",
    "panel.background": "#DDE3E4",
    "panel.border": "#BBBDBD",
    "panelTitle.activeForeground": "#877000",
    "panelTitle.inactiveForeground": "#A2A2A2",
    "scrollbarSlider.background": "#C7CDCE66",
    "scrollbarSlider.hoverBackground": "#BBBDBD77",
    "scrollbarSlider.activeBackground": "#BBBDBD99",
    "list.activeSelectionBackground": "#DDE3E4",
    "list.activeSelectionForeground": "#1D121E",
    "list.activeSelectionIconForeground": "#1D121E",
    "list.hoverBackground": "#D7D8DA",
    "list.hoverForeground": "#1D121E",
    "list.iconForeground": "#1D121E",
    "list.inactiveSelectionIconForeground": "#1D121E",
    "quickInputList.focusIconForeground": "#1D121E",
    "toolbar.hoverBackground": "#00000011",
    "toolbar.hoverForeground": "#1D121E",
    "toolbar.activeBackground": "#00000011",
    "toolbar.activeForeground": "#1D121E",
    "actionBar.toggledBackground": "#00000011",
    "menu.selectionBackground": "#E2BC072D",
    "menubar.selectionBackground": "#E2BC072D",
    "list.inactiveSelectionBackground": "#DDE3E4",
    "list.focusBackground": "#DDE3E4",
    "list.highlightForeground": "#877000",
    "notifications.background": "#DDE3E4",
    "notifications.foreground": "#877000",
    "notifications.border": "#BBBDBD",
    "notificationCenterHeader.background": "#C7CDCE",
    "notificationsInfoIcon.foreground": "#848280",
    "notificationsWarningIcon.foreground": "#BB9B37",
    "notificationsErrorIcon.foreground": "#EE5552",
    "icon.foreground": "#877000",
    "quickInput.background": "#DDE3E4",
    "quickInput.foreground": "#1D121E",
    "quickInput.list.focusBackground": "#C7CDCE",
    "focusBorder": "#E2BC07",
    "disabledForeground": "#848280",
    "descriptionForeground": "#A2A2A2",
    "textLink.foreground": "#E2BC07",
    "textLink.hoverForeground": "#E2BC07",
    "editorStickyScroll.shadow": "#C7CDCE",
    "statusBarItem.remoteForeground": "#1D121E",
    "statusBarItem.remoteBackground": "#E2BC07",
    "tooltip.background": "#C7CDCE",
    "tooltip.foreground": "#1D121E",
    "tooltip.border": "#BBBDBD",
    "keybindingTable.rowsBackground": "#C7CDCE",
    "panelSectionHeader.background": "#C7CDCE",
    "editorGroupHeader.tabsBackground": "#DDE3E4",
    "editorGroupHeader.noTabsBackground": "#DDE3E4"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line",
        "comment.line.double-slash",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#848280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#896904"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#94901B"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#A7ADAD"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": [
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#A7ADAD"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#7D6A38"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#E23935"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#7D6A38"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#1D121E"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#6A3296"
      }
    },
    {
      "name": "Class & Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#94901B"
      }
    },
    {
      "name": "Annotation / Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#6A3296"
      }
    },
    {
      "name": "Operator / Punctuation",
      "scope": [
        "punctuation",
        "operator"
      ],
      "settings": {
        "foreground": "#A7ADAD"
      }
    },
    {
      "name": "Invalid / Error",
      "scope": [
        "invalid",
        "invalid.deprecated",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E23935"
      }
    },
    {
      "name": "Property / Object Key",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#A25A00"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#896904"
      }
    },
    {
      "name": "Special Variable",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#A7ADAD"
      }
    },
    {
      "name": "Module / Namespace",
      "scope": [
        "entity.name.type.module",
        "support.other.module"
      ],
      "settings": {
        "foreground": "#94901B"
      }
    },
    {
      "name": "text.html.derivative",
      "scope": [
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#1D121E"
      }
    },
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "meta.tag.html",
        "meta.tag.xml",
        "meta.tag.other"
      ],
      "settings": {
        "foreground": "#896904"
      }
    },
    {
      "name": "HTML Attribute Name",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute",
        "meta.tag.xml.attribute"
      ],
      "settings": {
        "foreground": "#A25A00"
      }
    },
    {
      "name": "HTML Attribute Value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted.html"
      ],
      "settings": {
        "foreground": "#7D6A38"
      }
    },
    {
      "name": "HTML Punctuation",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag",
        "punctuation.separator.key-value.html"
      ],
      "settings": {
        "foreground": "#A7ADAD"
      }
    },
    {
      "name": "HTML Doctype / Comment",
      "scope": [
        "meta.tag.sgml.doctype",
        "comment.block.html"
      ],
      "settings": {
        "foreground": "#848280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX/TSX Tag",
      "scope": [
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#896904"
      }
    },
    {
      "name": "JSX Attribute",
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#A25A00"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "markup.heading.markdown",
        "markup.heading.setext",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#94901B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#6A3296",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#6A3296",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.title.markdown",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#6A3296",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Link URL",
      "scope": [
        "markup.underline.link",
        "markup.underline.link.image.markdown",
        "markup.underline.link.markdown",
        "markup.link.url.markdown"
      ],
      "settings": {
        "foreground": "#6A3296"
      }
    },
    {
      "name": "Markdown Code Inline",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.raw.inline.markdown"
      ],
      "settings": {
        "foreground": "#7D6A38"
      }
    },
    {
      "name": "Markdown Code Block",
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.raw.block.markdown",
        "fenced_code.block.language",
        "fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#7D6A38"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#FFDB2B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List / Bullet",
      "scope": [
        "markup.list",
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#7D6A38"
      }
    },
    {
      "name": "Markdown Separator / HR",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "#A7ADAD"
      }
    },
    {
      "name": "Markdown Comment (HTML-style)",
      "scope": [
        "comment.block.html.markdown"
      ],
      "settings": {
        "foreground": "#848280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Heading 1 (#)",
      "scope": [
        "punctuation.definition.heading.markdown",
        "heading.1.markdown",
        "markup.heading.markdown",
        "markup.heading.setext.markdown",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#94901B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#7D6A38"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#7D6A38"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "meta.object-literal.key",
        "meta.object.member",
        "meta.objectliteral",
        "support.type.property-name",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#A25A00"
      }
    },
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "variable.other.property.js",
        "variable.other.object.property",
        "support.variable.property.dom",
        "meta.property.object.js"
      ],
      "settings": {
        "foreground": "#A25A00"
      }
    },
    {
      "name": "Method (dot chain)",
      "scope": [
        "support.function",
        "variable.function",
        "variable.function.js",
        "entity.name.function.member",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#6A3296"
      }
    },
    {
      "name": "Object variable (req, res, fs, helper, etc.)",
      "scope": [
        "variable.other.object.js",
        "variable.other.object.js.jsx"
      ],
      "settings": {
        "foreground": "#1D121E"
      }
    },
    {
      "name": "Promise",
      "scope": [
        "support.class.promise.js",
        "support.class.promise.ts"
      ],
      "settings": {
        "foreground": "#A7ADAD"
      }
    },
    {
      "name": "Diff Header (from-file)",
      "scope": [
        "meta.diff.header.from-file",
        "punctuation.definition.from-file.diff"
      ],
      "settings": {
        "foreground": "#E23935",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Header (to-file)",
      "scope": [
        "meta.diff.header.to-file",
        "punctuation.definition.to-file.diff"
      ],
      "settings": {
        "foreground": "#896904",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Range / Index Line",
      "scope": [
        "meta.diff.range",
        "meta.diff.index"
      ],
      "settings": {
        "foreground": "#94901B"
      }
    },
    {
      "name": "Diff Added Line",
      "scope": [
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#896904"
      }
    },
    {
      "name": "Diff Removed Line",
      "scope": [
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#E23935"
      }
    },
    {
      "name": "Diff Changed Line",
      "scope": [
        "markup.changed.diff"
      ],
      "settings": {
        "foreground": "#94901B"
      }
    },
    {
      "name": "Diff Context Line",
      "scope": [
        "meta.diff.context",
        "meta.diff"
      ],
      "settings": {
        "foreground": "#848280"
      }
    },
    {
      "name": "Diff Range Punctuation",
      "scope": [
        "punctuation.definition.range.diff"
      ],
      "settings": {
        "foreground": "#94901B"
      }
    },
    {
      "name": "Diff Meta Info",
      "scope": [
        "meta.diff.header",
        "meta.separator.diff"
      ],
      "settings": {
        "foreground": "#A25A00"
      }
    },
    {
      "name": "Diff Inserted Punctuation",
      "scope": [
        "punctuation.definition.inserted.diff"
      ],
      "settings": {
        "foreground": "#94901B"
      }
    },
    {
      "name": "CSS",
      "scope": [
        "support.function.misc.css",
        "meta.function.variable.css"
      ],
      "settings": {
        "foreground": "#6A3296"
      }
    }
  ]
}
