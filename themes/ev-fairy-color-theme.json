    "tab.hoverBackground": "#F7F9FB",
    "tab.editor-group-tab-background": "#FFF6F7",
    "tab.border": "#F49EB033",
    "tab.activeBorder": "#00000000",
    "tab.activeBorderTop": "#00000000",
    "tab.unfocusedActiveBorder": "#00000000",
    "tab.unfocusedActiveBorderTop": "#00000000",
    "editorGroupHeader.tabsBorder": "#00000000",
    "terminal.background": "#FFF6F7",
    "terminal.foreground": "#43384C",
    "terminal.ansiBlack": "#CDB1C1",
    "terminal.ansiRed": "#E56D90",
    "terminal.ansiGreen": "#6595CF",
    "terminal.ansiYellow": "#F6BFC4",
    "terminalStickyScroll.background": "#F6DFE6",
    "terminal.ansiBlue": "#637BB1",
    "terminal.ansiMagenta": "#F19DAF",
    "terminal.ansiCyan": "#B5E0F3",
    "terminal.ansiWhite": "#F7F9FB",
    "terminal.ansiBrightBlack": "#E6CFD9",
    "terminal.ansiBrightRed": "#F19DAF",
    "terminal.ansiBrightGreen": "#7EA7D8",
    "terminal.ansiBrightYellow": "#FFF6F7",
    "terminal.ansiBrightBlue": "#6595CF",
    "terminal.ansiBrightMagenta": "#F6BFC4",
    "terminal.ansiBrightCyan": "#B5E0F3",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "badge.background": "#FFFFFF",
    "badge.foreground": "#F19DAF",
    "button.background": "#637BB1",
    "button.hoverBackground": "#FF049319",
    "button.foreground": "#FFFFFF",
    "button.secondaryBackground": "#F7F9FB",
    "button.secondaryHoverBackground": "#F6BFC4",
    "button.secondaryForeground": "#43384C",
    "extensionButton.background": "#637BB1",
    "extensionButton.foreground": "#FFFFFF",
    "extensionButton.hoverBackground": "#6595CF",
    "extensionButton.prominentBackground": "#F19DAF",
    "extensionButton.prominentForeground": "#FFFFFF",
    "extensionButton.prominentHoverBackground": "#F6BFC4",
    "editorWidget.foreground": "#43384C",
    "editorWidget.background": "#FCFEFF",
    "input.background": "#FCFEFF",
    "input.foreground": "#4779B6",
    "input.border": "#FCFEFF",
    "input.placeholderForeground": "#CDB1C1",
    "dropdown.background": "#ffffff",
    "dropdown.foreground": "#43384C",
    "dropdown.border": "#E0C2CE",
    "widget.shadow": "#9F8EA266",
    "scrollbar.shadow": "#9F8EA233",
    "panel.background": "#FFF6F7",
    "panel.border": "#F49EB033",
    "panelTitle.activeForeground": "#4779B6",
    "panelTitle.inactiveForeground": "#BFA7B6",
    "scrollbarSlider.background": "#CDB1C144",
    "scrollbarSlider.hoverBackground": "#CDB1C166",
    "scrollbarSlider.activeBackground": "#CDB1C188",
    "list.activeSelectionBackground": "#F19DAF33",
    "list.activeSelectionForeground": "#43384C",
    "list.activeSelectionIconForeground": "#43384C",
    "list.hoverBackground": "#ffffff",
    "list.hoverForeground": "#43384C",
    "list.iconForeground": "#43384C",
    "list.inactiveSelectionIconForeground": "#43384C",
    "quickInputList.focusIconForeground": "#43384C",
    "toolbar.hoverBackground": "#FF049319",
    "toolbar.hoverForeground": "#43384C",
    "toolbar.activeBackground": "#FFF6F7",
    "toolbar.activeForeground": "#43384C",
    "actionBar.toggledBackground": "#FF049319",
    "menu.selectionBackground": "#FF049319",
    "menubar.selectionBackground": "#FF049319",
    "list.inactiveSelectionBackground": "#F19DAF33",
    "list.focusBackground": "#F19DAF33",
    "list.focusOutline": "#00000000",
    "list.inactiveFocusOutline": "#00000000",
    "list.highlightForeground": "#637BB1",
    "notifications.background": "#FCFEFF",
    "notifications.foreground": "#4779B6",
    "notifications.border": "#E0C2CE",
    "notificationCenterHeader.background": "#F6DFE6",
    "notificationsInfoIcon.foreground": "#6595CF",
    "notificationsWarningIcon.foreground": "#F6BFC4",
    "notificationsErrorIcon.foreground": "#E56D90",
    "icon.foreground": "#637BB1",
    "quickInput.background": "#FCFEFF",
    "quickInput.foreground": "#43384C",
    "quickInput.list.focusBackground": "#F6BFC466",
    "focusBorder": "#637BB1",
    "disabledForeground": "#9F8EA2",
    "descriptionForeground": "#43384C",
    "textLink.foreground": "#637BB1",
    "textLink.hoverForeground": "#637BB1",
    "editorStickyScroll.shadow": "#F19DAF33",
    "statusBarItem.remoteForeground": "#FFFFFF",
    "statusBarItem.remoteBackground": "#6595CF",
    "tooltip.background": "#FFFFFF",
    "tooltip.foreground": "#43384C",
    "tooltip.border": "#FFFFFF",
    "editorHoverWidget.background": "#FFFFFF",
    "editorHoverWidget.foreground": "#43384C",
    "editorHoverWidget.border": "#FFFFFF",
    "editorHoverWidget.statusBarBackground": "#FFFFFF",
    "keybindingTable.rowsBackground": "#F19DAF33",
    "panelSectionHeader.background": "#F19DAF33",
    "editorGroupHeader.tabsBackground": "#FFF6F7",
    "editorGroupHeader.noTabsBackground": "#FFF6F7"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line",
        "comment.line.double-slash",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#CBB2C3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#2F4FA8"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#4A71B5"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": [
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#2F4FA8"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#D12D64"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#C54572"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#4A3F50"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "Class & Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#4A71B5"
      }
    },
    {
      "name": "Annotation / Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "Operator / Punctuation",
      "scope": [
        "punctuation",
        "operator"
      ],
      "settings": {
        "foreground": "#4A71B5"
      }
    },
    {
      "name": "Invalid / Error",
      "scope": [
        "invalid",
        "invalid.deprecated",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#F19DAF"
      }
    },
    {
      "name": "Property / Object Key",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#2F4FA8"
      }
    },
    {
      "name": "Special Variable",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#2F4FA8"
      }
    },
    {
      "name": "Module / Namespace",
      "scope": [
        "entity.name.type.module",
        "support.other.module"
      ],
      "settings": {
        "foreground": "#2F4FA8"
      }
    },
    {
      "name": "text.html.derivative",
      "scope": [
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#4A3F50"
      }
    },
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "meta.tag.html",
        "meta.tag.xml",
        "meta.tag.other"
      ],
      "settings": {
        "foreground": "#2F4FA8"
      }
    },
    {
      "name": "HTML Attribute Name",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute",
        "meta.tag.xml.attribute"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "HTML Attribute Value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted.html"
      ],
      "settings": {
        "foreground": "#D12D64"
      }
    },
    {
      "name": "HTML Punctuation",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag",
        "punctuation.separator.key-value.html"
      ],
      "settings": {
        "foreground": "#3C6AB4"
      }
    },
    {
      "name": "HTML Doctype / Comment",
      "scope": [
        "meta.tag.sgml.doctype",
        "comment.block.html"
      ],
      "settings": {
        "foreground": "#CBB2C3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX/TSX Tag",
      "scope": [
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#2F4FA8"
      }
    },
    {
      "name": "JSX Attribute",
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "markup.heading.markdown",
        "markup.heading.setext",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#2F4FA8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#F19DAF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#F19DAF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.title.markdown",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#2081C4",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Link URL",
      "scope": [
        "markup.underline.link",
        "markup.underline.link.image.markdown",
        "markup.underline.link.markdown",
        "markup.link.url.markdown"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "Markdown Code Inline",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.raw.inline.markdown"
      ],
      "settings": {
        "foreground": "#C54572"
      }
    },
    {
      "name": "Markdown Code Block",
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.raw.block.markdown",
        "fenced_code.block.language",
        "fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#C54572"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#F7F0F3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List / Bullet",
      "scope": [
        "markup.list",
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#4A71B5"
      }
    },
    {
      "name": "Markdown Separator / HR",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "#4A71B5"
      }
    },
    {
      "name": "Markdown Comment (HTML-style)",
      "scope": [
        "comment.block.html.markdown"
      ],
      "settings": {
        "foreground": "#CBB2C3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Heading 1 (#)",
      "scope": [
        "punctuation.definition.heading.markdown",
        "heading.1.markdown",
        "markup.heading.markdown",
        "markup.heading.setext.markdown",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#2F4FA8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#4A71B5"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#4A71B5"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "meta.object-literal.key",
        "meta.object.member",
        "meta.objectliteral",
        "support.type.property-name",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "variable.other.property.js",
        "variable.other.object.property",
        "support.variable.property.dom",
        "meta.property.object.js"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "Method (dot chain)",
      "scope": [
        "support.function",
        "variable.function",
        "variable.function.js",
        "entity.name.function.member",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#4A71B5"
      }
    },
    {
      "name": "Object variable (req, res, fs, helper, etc.)",
      "scope": [
        "variable.other.object.js",
        "variable.other.object.js.jsx"
      ],
      "settings": {
        "foreground": "#4A3F50"
      }
    },
    {
      "name": "Promise",
      "scope": [
        "support.class.promise.js",
        "support.class.promise.ts"
      ],
      "settings": {
        "foreground": "#2F4FA8"
      }
    },
    {
      "name": "Diff Header (from-file)",
      "scope": [
        "meta.diff.header.from-file",
        "punctuation.definition.from-file.diff"
      ],
      "settings": {
        "foreground": "#F19DAF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Header (to-file)",
      "scope": [
        "meta.diff.header.to-file",
        "punctuation.definition.to-file.diff"
      ],
      "settings": {
        "foreground": "#2081C4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Range / Index Line",
      "scope": [
        "meta.diff.range",
        "meta.diff.index"
      ],
      "settings": {
        "foreground": "#CDB1C1"
      }
    },
    {
      "name": "Diff Added Line",
      "scope": [
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "Diff Removed Line",
      "scope": [
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#F19DAF"
      }
    },
    {
      "name": "Diff Changed Line",
      "scope": [
        "markup.changed.diff"
      ],
      "settings": {
        "foreground": "#F6BFC4"
      }
    },
    {
      "name": "Diff Context Line",
      "scope": [
        "meta.diff.context",
        "meta.diff"
      ],
      "settings": {
        "foreground": "#7B93B8"
      }
    },
    {
      "name": "Diff Range Punctuation",
      "scope": [
        "punctuation.definition.range.diff"
      ],
      "settings": {
        "foreground": "#CDB1C1"
      }
    },
    {
      "name": "Diff Meta Info",
      "scope": [
        "meta.diff.header",
        "meta.separator.diff"
      ],
      "settings": {
        "foreground": "#2F4FA8"
      }
    },
    {
      "name": "Diff Inserted Punctuation",
      "scope": [
        "punctuation.definition.inserted.diff"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    },
    {
      "name": "CSS",
      "scope": [
        "support.function.misc.css",
        "meta.function.variable.css"
      ],
      "settings": {
        "foreground": "#2081C4"
      }
    }
  ]
}
