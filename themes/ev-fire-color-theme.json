{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ev Fire",
  "type": "light",
  "colors": {
    "foreground": "#4A3540",
    "editor.background": "#FCFAED",
    "editor.foreground": "#4A3540",
    "editorLineNumber.foreground": "#7C6472",
    "editorLineNumber.activeForeground": "#B63851",
    "editorCursor.foreground": "#E77E3F",
    "editor.selectionBackground": "#FCEECF",
    "editor.inactiveSelectionBackground": "#FFF9E5",
    "editor.lineHighlightBackground": "#FFF9E5",
    "editorWhitespace.foreground": "#E0D5B4",
    "editorIndentGuide.background1": "#E0D5B4",
    "editorIndentGuide.activeBackground1": "#7C6472",
    "activityBar.background": "#E77E3F",
    "activityBar.foreground": "#FFF8CC",
    "activityBar.inactiveForeground": "#FFF8CC88",
    "activityBar.activeBorder": "#FFF8CC",
    "activityBarBadge.background": "#A74100",
    "activityBarBadge.foreground": "#FFF7C7",
    "sideBar.background": "#F7EFD6",
    "sideBar.foreground": "#4A3540",
    "sideBarSectionHeader.background": "#FCFAED",
    "sideBarSectionHeader.foreground": "#4A3540",
    "statusBar.background": "#E77E3F",
    "statusBar.foreground": "#FFF7C7",
    "statusBar.debuggingBackground": "#F49757",
    "statusBar.debuggingForeground": "#FFF7C7",
    "statusBar.noFolderBackground": "#FFFDF3",
    "titleBar.activeBackground": "#E77E3F",
    "titleBar.inactiveBackground": "#E77E3F",
    "commandCenter.foreground": "#FFF7C7",
    "commandCenter.activeForeground": "#FFF7C7",
    "commandCenter.inactiveForeground": "#FFF7C7",
    "tab.activeBackground": "#FCFAED",
    "tab.activeForeground": "#3E303A",
    "tab.inactiveBackground": "#F7EFD6",
    "tab.inactiveForeground": "#9D9989",
    "tab.hoverBackground": "#FCFAED",
    "tab.editor-group-tab-background": "#F7EFD6",
    "tab.border": "#D8C9A6",
    "terminal.background": "#F7EFD6",
    "terminal.foreground": "#4A3540",
    "terminal.ansiBlack": "#E0D5B4",
    "terminal.ansiRed": "#B63851",
    "terminal.ansiGreen": "#487AA9",
    "terminal.ansiYellow": "#F0E49A",
    "terminalStickyScroll.background": "#F1E6CD",
    "terminal.ansiBlue": "#5D5094",
    "terminal.ansiMagenta": "#B55C77",
    "terminal.ansiCyan": "#6A5EA3",
    "terminal.ansiWhite": "#7C6472",
    "terminal.ansiBrightBlack": "#9B8A95",
    "terminal.ansiBrightRed": "#C8646D",
    "terminal.ansiBrightGreen": "#6793BA",
    "terminal.ansiBrightYellow": "#F7EEA8",
    "terminal.ansiBrightBlue": "#6A5EA3",
    "terminal.ansiBrightMagenta": "#CFA0B5",
    "terminal.ansiBrightCyan": "#83A5C5",
    "terminal.ansiBrightWhite": "#FFF7C7",
    "badge.background": "#B63851",
    "badge.foreground": "#FFF7C7",
    "button.background": "#E77E3F",
    "button.hoverBackground": "#F49757",
    "button.foreground": "#FFFDF3",
    "button.secondaryBackground": "#FFFDF3",
    "button.secondaryHoverBackground": "#FFF9E5",
    "button.secondaryForeground": "#4A3540",
    "descriptionForeground": "#817369",
    "titleBar.activeForeground": "#FFFDF3",
    "titleBar.inactiveForeground": "#FFFDF3",
    "extensionButton.background": "#E77E3F",
    "extensionButton.foreground": "#FFF7C7",
    "extensionButton.hoverBackground": "#F49757",
    "extensionButton.prominentBackground": "#E77E3F",
    "extensionButton.prominentForeground": "#FFF7C7",
    "extensionButton.prominentHoverBackground": "#F49757",
    "editorWidget.foreground": "#4A3540",
    "editorWidget.background": "#FFFDF3",
    "input.background": "#FFFDF3",
    "input.foreground": "#4A3540",
    "input.border": "#FFFDF3",
    "input.placeholderForeground": "#9B8A95",
    "dropdown.background": "#F7EFD6",
    "dropdown.foreground": "#4A3540",
    "dropdown.border": "#D8C9A6",
    "widget.shadow": "#7C647266",
    "scrollbar.shadow": "#92857D33",
    "panel.background": "#F7EFD6",
    "panel.border": "#D8C9A6",
    "panelTitle.activeForeground": "#4A3540",
    "panelTitle.inactiveForeground": "#4A3540",
    "scrollbarSlider.background": "#E0D5B466",
    "scrollbarSlider.hoverBackground": "#D8C9A677",
    "scrollbarSlider.activeBackground": "#D8C9A688",
    "list.activeSelectionBackground": "#FCEECF",
    "list.activeSelectionForeground": "#3E303A",
    "list.hoverBackground": "#FFF9E5",
    "list.hoverForeground": "#3E303A",
    "list.inactiveSelectionBackground": "#FFFDF3",
    "list.focusBackground": "#FCEECF",
    "list.highlightForeground": "#E77E3F",
    "notifications.background": "#FFFDF3",
    "notifications.foreground": "#3E303A",
    "notifications.border": "#D8C9A6",
    "notificationCenterHeader.background": "#E77E3F",
    "notificationsInfoIcon.foreground": "#487AA9",
    "notificationsWarningIcon.foreground": "#B63851",
    "notificationsErrorIcon.foreground": "#B63851",
    "toolbar.hoverBackground": "#00000011",
    "toolbar.hoverForeground": "#4A3540",
    "toolbar.inactiveForeground": "#FFF8CC88",
    "toolbar.activeBackground": "#00000011",
    "toolbar.activeForeground": "#4A3540",
    "actionBar.toggledBackground": "#00000011",
    "actionBar.toggledForeground": "#4A3540",
    "menu.selectionBackground": "#E9D7C055",
    "menu.selectionForeground": "#4A3540",
    "menubar.selectionBackground": "#E9D7C055",
    "menubar.selectionForeground": "#4A3540",
    "quickInput.background": "#FFFDF3",
    "quickInput.foreground": "#4A3540",
    "quickInput.hoverForeground": "#cc0000",
    "quickInput.list.focusBackground": "#FCEECF",
    "focusBorder": "#E77E3F",
    "textLink.foreground": "#5D5094",
    "textLink.hoverForeground": "#5D5094",
    "statusBarItem.remoteForeground": "#FFF7C7",
    "statusBarItem.remoteBackground": "#E77E3F",
    "keybindingTable.rowsBackground": "#F1E6CD",
    "panelSectionHeader.background": "#FCFAED",
    "editorGroupHeader.tabsBackground": "#F7EFD6",
    "editorGroupHeader.noTabsBackground": "#F7EFD6"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line",
        "comment.line.double-slash",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6C5142",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#D25F1D"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#E07735"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#967963"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": [
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#967963"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#D25F1D"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#B63851"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#C6915A"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#1E4C31"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#0F7A55"
      }
    },
    {
      "name": "Class & Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Annotation / Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#E07735"
      }
    },
    {
      "name": "Operator / Punctuation",
      "scope": [
        "punctuation",
        "operator"
      ],
      "settings": {
        "foreground": "#967963"
      }
    },
    {
      "name": "Invalid / Error",
      "scope": [
        "invalid",
        "invalid.deprecated",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#B63851"
      }
    },
    {
      "name": "Property / Object Key",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Special Variable",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Module / Namespace",
      "scope": [
        "entity.name.type.module",
        "support.other.module"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "text.html.derivative",
      "scope": [
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#1E4C31"
      }
    },
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "meta.tag.html",
        "meta.tag.xml",
        "meta.tag.other"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "HTML Attribute Name",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute",
        "meta.tag.xml.attribute"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "HTML Attribute Value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted.html"
      ],
      "settings": {
        "foreground": "#C6915A"
      }
    },
    {
      "name": "HTML Punctuation",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag",
        "punctuation.separator.key-value.html"
      ],
      "settings": {
        "foreground": "#967963"
      }
    },
    {
      "name": "HTML Doctype / Comment",
      "scope": [
        "meta.tag.sgml.doctype",
        "comment.block.html"
      ],
      "settings": {
        "foreground": "#6C5142",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX/TSX Tag",
      "scope": [
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "JSX Attribute",
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "markup.heading.markdown",
        "markup.heading.setext",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#D25F1D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#B63851",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#B63851",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.title.markdown",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#1C7F5A",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Link URL",
      "scope": [
        "markup.underline.link",
        "markup.underline.link.image.markdown",
        "markup.underline.link.markdown",
        "markup.link.url.markdown"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Markdown Code Inline",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.raw.inline.markdown"
      ],
      "settings": {
        "foreground": "#C6915A"
      }
    },
    {
      "name": "Markdown Code Block",
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.raw.block.markdown",
        "fenced_code.block.language",
        "fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#C6915A"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#F2D3B2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List / Bullet",
      "scope": [
        "markup.list",
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#6C5142"
      }
    },
    {
      "name": "Markdown Separator / HR",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "#967963"
      }
    },
    {
      "name": "Markdown Comment (HTML-style)",
      "scope": [
        "comment.block.html.markdown"
      ],
      "settings": {
        "foreground": "#6C5142",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Heading 1 (#)",
      "scope": [
        "punctuation.definition.heading.markdown",
        "heading.1.markdown",
        "markup.heading.markdown",
        "markup.heading.setext.markdown",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#D25F1D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#6C5142"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#6C5142"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "meta.object-literal.key",
        "meta.object.member",
        "meta.objectliteral",
        "support.type.property-name",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "variable.other.property.js",
        "variable.other.object.property",
        "support.variable.property.dom",
        "meta.property.object.js"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Method (dot chain)",
      "scope": [
        "support.function",
        "variable.function",
        "variable.function.js",
        "entity.name.function.member",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#0F7A55"
      }
    },
    {
      "name": "Object variable (req, res, fs, helper, etc.)",
      "scope": [
        "variable.other.object.js",
        "variable.other.object.js.jsx"
      ],
      "settings": {
        "foreground": "#1E4C31"
      }
    },
    {
      "name": "Promise",
      "scope": [
        "support.class.promise.js",
        "support.class.promise.ts"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Diff Header (from-file)",
      "scope": [
        "meta.diff.header.from-file",
        "punctuation.definition.from-file.diff"
      ],
      "settings": {
        "foreground": "#B63851",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Header (to-file)",
      "scope": [
        "meta.diff.header.to-file",
        "punctuation.definition.to-file.diff"
      ],
      "settings": {
        "foreground": "#0F7A55",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Range / Index Line",
      "scope": [
        "meta.diff.range",
        "meta.diff.index"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Diff Added Line",
      "scope": [
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#0F7A55"
      }
    },
    {
      "name": "Diff Removed Line",
      "scope": [
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#B63851"
      }
    },
    {
      "name": "Diff Changed Line",
      "scope": [
        "markup.changed.diff"
      ],
      "settings": {
        "foreground": "#D25F1D"
      }
    },
    {
      "name": "Diff Context Line",
      "scope": [
        "meta.diff.context",
        "meta.diff"
      ],
      "settings": {
        "foreground": "#6C5142"
      }
    },
    {
      "name": "Diff Range Punctuation",
      "scope": [
        "punctuation.definition.range.diff"
      ],
      "settings": {
        "foreground": "#1C7F5A"
      }
    },
    {
      "name": "Diff Meta Info",
      "scope": [
        "meta.diff.header",
        "meta.separator.diff"
      ],
      "settings": {
        "foreground": "#D25F1D"
      }
    },
    {
      "name": "Diff Inserted Punctuation",
      "scope": [
        "punctuation.definition.inserted.diff"
      ],
      "settings": {
        "foreground": "#0F7A55"
      }
    },
    {
      "name": "CSS",
      "scope": [
        "support.function.misc.css",
        "meta.function.variable.css"
      ],
      "settings": {
        "foreground": "#0F7A55"
      }
    }
  ]
}
