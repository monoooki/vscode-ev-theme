    "sideBar.background": "#FAF5E6",
    "sideBar.foreground": "#633F17",
    "sideBarSectionHeader.background": "#D9B68F66",
    "sideBarSectionHeader.foreground": "#936028",
    "sideBarSectionHeader.border": "#CAB886",
    "statusBar.background": "#5A9D72",
    "statusBar.foreground": "#FFFCF1",
    "statusBar.debuggingBackground": "#7CC597",
    "statusBar.debuggingForeground": "#FFFCF1",
    "statusBar.noFolderBackground": "#FFFCF1",
    "titleBar.activeBackground": "#5A9D72",
    "titleBar.activeForeground": "#FFFCF1",
    "titleBar.inactiveBackground": "#5A9D72",
    "titleBar.inactiveForeground": "#FFFCF1",
    "commandCenter.foreground": "#FFFCF1",
    "commandCenter.activeForeground": "#FFFCF1",
    "commandCenter.inactiveForeground": "#FFFCF1",
    "tab.activeBackground": "#FFFCF1",
    "tab.activeForeground": "#AD6A00",
    "tab.inactiveBackground": "#FAF5E6",
    "tab.inactiveForeground": "#90A87D",
    "tab.hoverBackground": "#FFFCF1",
    "tab.editor-group-tab-background": "#FAF5E6",
    "tab.border": "#CAB886",
    "tab.activeBorder": "#00000000",
    "tab.activeBorderTop": "#00000000",
    "tab.unfocusedActiveBorder": "#00000000",
    "tab.unfocusedActiveBorderTop": "#00000000",
    "editorGroupHeader.tabsBorder": "#CAB886",
    "terminal.background": "#FFFCF1",
    "terminal.foreground": "#936028",
    "terminal.ansiBlack": "#FCEAAF",
    "terminal.ansiRed": "#AD6A00",
    "terminal.ansiGreen": "#7CC597",
    "terminal.ansiYellow": "#FCEAAF",
    "terminalStickyScroll.background": "#FAF5E6",
    "terminal.ansiBlue": "#90A87D",
    "terminal.ansiMagenta": "#CAB886",
    "terminal.ansiCyan": "#AAD6B1",
    "terminal.ansiWhite": "#90A87D",
    "terminal.ansiBrightBlack": "#CAB886",
    "terminal.ansiBrightRed": "#936028",
    "terminal.ansiBrightGreen": "#AAD6B1",
    "terminal.ansiBrightYellow": "#FCEAAF",
    "terminal.ansiBrightBlue": "#AAD6B1",
    "terminal.ansiBrightMagenta": "#FCEAAF",
    "terminal.ansiBrightCyan": "#AAD6B1",
    "terminal.ansiBrightWhite": "#FFFCF1",
    "badge.background": "#835100",
    "badge.foreground": "#FFFCF1",
    "button.background": "#5A9D72",
    "button.hoverBackground": "#7CC597",
    "button.foreground": "#FFFCF1",
    "button.secondaryBackground": "#FFFCF1",
    "button.secondaryHoverBackground": "#FCEAAF",
    "button.secondaryForeground": "#936028",
    "extensionButton.background": "#5A9D72",
    "extensionButton.foreground": "#FFFCF1",
    "extensionButton.hoverBackground": "#7CC597",
    "extensionButton.prominentBackground": "#5A9D72",
    "extensionButton.prominentForeground": "#FFFCF1",
    "extensionButton.prominentHoverBackground": "#7CC597",
    "editorWidget.foreground": "#936028",
    "editorWidget.background": "#FFFCF1",
    "input.background": "#FFFCF1",
    "input.foreground": "#936028",
    "input.border": "#FFFCF1",
    "input.placeholderForeground": "#90A87D",
    "dropdown.background": "#ffffff",
    "dropdown.foreground": "#936028",
    "dropdown.border": "#CAB886",
    "widget.shadow": "#90A87D66",
    "scrollbar.shadow": "#90A87D33",
    "panel.background": "#FAF5E6",
    "panel.foreground": "#633F17",
    "panel.border": "#CAB886",
    "panelTitle.activeForeground": "#633F17",
    "panelTitle.inactiveForeground": "#633F17",
    "scrollbarSlider.background": "#CAB88666",
    "scrollbarSlider.hoverBackground": "#CAB88677",
    "scrollbarSlider.activeBackground": "#CAB88688",
    "list.foreground": "#633F17",
    "list.activeSelectionBackground": "#FCEAAF",
    "list.activeSelectionForeground": "#633F17",
    "list.activeSelectionIconForeground": "#633F17",
    "list.hoverBackground": "#FCEAAF33",
    "list.hoverForeground": "#633F17",
    "list.iconForeground": "#633F17",
    "list.inactiveSelectionForeground": "#633F17",
    "list.inactiveSelectionIconForeground": "#633F17",
    "list.focusForeground": "#633F17",
    "quickInputList.focusIconForeground": "#633F17",
    "toolbar.hoverBackground": "#00000011",
    "toolbar.hoverForeground": "#936028",
    "toolbar.activeBackground": "#00000011",
    "toolbar.activeForeground": "#936028",
    "actionBar.toggledBackground": "#00000011",
    "menu.selectionBackground": "#5A9D7233",
    "menubar.selectionBackground": "#5A9D7233",
    "list.inactiveSelectionBackground": "#FFFCF1",
    "list.focusBackground": "#FCEAAF",
    "list.highlightForeground": "#AD6A00",
    "notifications.background": "#FFFCF1",
    "notifications.foreground": "#633F17",
    "notifications.border": "#CAB886",
    "notificationCenterHeader.background": "#AAD6B1",
    "notificationsInfoIcon.foreground": "#90A87D",
    "notificationsWarningIcon.foreground": "#AD6A00",
    "notificationsErrorIcon.foreground": "#AD6A00",
    "icon.foreground": "#633F17",
    "quickInput.background": "#FFFCF1",
    "quickInput.foreground": "#936028",
    "quickInput.list.focusBackground": "#FCEAAF",
    "focusBorder": "#5A9D72",
    "disabledForeground": "#90A87D",
    "descriptionForeground": "#936028",
    "textLink.foreground": "#5A9D72",
    "textLink.hoverForeground": "#5A9D72",
    "editorStickyScroll.shadow": "#CAB886",
    "statusBarItem.remoteForeground": "#FFFCF1",
    "statusBarItem.remoteBackground": "#5A9D72",
    "tooltip.background": "#FCEAAF",
    "tooltip.foreground": "#936028",
    "tooltip.border": "#CAB886",
    "keybindingTable.rowsBackground": "#CAB886",
    "panelSectionHeader.background": "#D9B68F66",
    "editorGroupHeader.tabsBackground": "#FFFCF1",
    "editorGroupHeader.noTabsBackground": "#FFFCF1"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line",
        "comment.line.double-slash",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#B1C1B7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#277130"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#4F8D53"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#B59F7B"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": [
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#B59F7B"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#4F8D53"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#7A3F00"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#AD6A00"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#7A3F00"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#2F6F68"
      }
    },
    {
      "name": "Class & Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#58A49E"
      }
    },
    {
      "name": "Annotation / Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#4F8D53"
      }
    },
    {
      "name": "Operator / Punctuation",
      "scope": [
        "punctuation",
        "operator"
      ],
      "settings": {
        "foreground": "#B59F7B"
      }
    },
    {
      "name": "Invalid / Error",
      "scope": [
        "invalid",
        "invalid.deprecated",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#AD6A00"
      }
    },
    {
      "name": "Property / Object Key",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#277130"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#277130"
      }
    },
    {
      "name": "Special Variable",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#B59F7B"
      }
    },
    {
      "name": "Module / Namespace",
      "scope": [
        "entity.name.type.module",
        "support.other.module"
      ],
      "settings": {
        "foreground": "#58A49E"
      }
    },
    {
      "name": "text.html.derivative",
      "scope": [
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#936028"
      }
    },
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "meta.tag.html",
        "meta.tag.xml",
        "meta.tag.other"
      ],
      "settings": {
        "foreground": "#277130"
      }
    },
    {
      "name": "HTML Attribute Name",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute",
        "meta.tag.xml.attribute"
      ],
      "settings": {
        "foreground": "#3F8B84"
      }
    },
    {
      "name": "HTML Attribute Value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted.html"
      ],
      "settings": {
        "foreground": "#AD6A00"
      }
    },
    {
      "name": "HTML Punctuation",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag",
        "punctuation.separator.key-value.html"
      ],
      "settings": {
        "foreground": "#8C774E"
      }
    },
    {
      "name": "HTML Doctype / Comment",
      "scope": [
        "meta.tag.sgml.doctype",
        "comment.block.html"
      ],
      "settings": {
        "foreground": "#B1C1B7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX/TSX Tag",
      "scope": [
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#277130"
      }
    },
    {
      "name": "JSX Attribute",
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#3F8B84"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "markup.heading.markdown",
        "markup.heading.setext",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#277130",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#AD6A00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#AD6A00",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.title.markdown",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#277130",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Link URL",
      "scope": [
        "markup.underline.link",
        "markup.underline.link.image.markdown",
        "markup.underline.link.markdown",
        "markup.link.url.markdown"
      ],
      "settings": {
        "foreground": "#3F8B84"
      }
    },
    {
      "name": "Markdown Code Inline",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.raw.inline.markdown"
      ],
      "settings": {
        "foreground": "#AD6A00"
      }
    },
    {
      "name": "Markdown Code Block",
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.raw.block.markdown",
        "fenced_code.block.language",
        "fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#AD6A00"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#C4AD82",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List / Bullet",
      "scope": [
        "markup.list",
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#8C774E"
      }
    },
    {
      "name": "Markdown Separator / HR",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "#8C774E"
      }
    },
    {
      "name": "Markdown Comment (HTML-style)",
      "scope": [
        "comment.block.html.markdown"
      ],
      "settings": {
        "foreground": "#B1C1B7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Heading 1 (#)",
      "scope": [
        "punctuation.definition.heading.markdown",
        "heading.1.markdown",
        "markup.heading.markdown",
        "markup.heading.setext.markdown",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#277130",
        "fontStyle": "bold"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#8C774E"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#8C774E"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "meta.object-literal.key",
        "meta.object.member",
        "meta.objectliteral",
        "support.type.property-name",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#277130"
      }
    },
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "variable.other.property.js",
        "variable.other.object.property",
        "support.variable.property.dom",
        "meta.property.object.js"
      ],
      "settings": {
        "foreground": "#277130"
      }
    },
    {
      "name": "Method (dot chain)",
      "scope": [
        "support.function",
        "variable.function",
        "variable.function.js",
        "entity.name.function.member",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#3F8B84"
      }
    },
    {
      "name": "Object variable (req, res, fs, helper, etc.)",
      "scope": [
        "variable.other.object.js",
        "variable.other.object.js.jsx"
      ],
      "settings": {
        "foreground": "#7A3F00"
      }
    },
    {
      "name": "Promise",
      "scope": [
        "support.class.promise.js",
        "support.class.promise.ts"
      ],
      "settings": {
        "foreground": "#B59F7B"
      }
    },
    {
      "name": "Diff Header (from-file)",
      "scope": [
        "meta.diff.header.from-file",
        "punctuation.definition.from-file.diff"
      ],
      "settings": {
        "foreground": "#AD6A00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Header (to-file)",
      "scope": [
        "meta.diff.header.to-file",
        "punctuation.definition.to-file.diff"
      ],
      "settings": {
        "foreground": "#58A49E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Range / Index Line",
      "scope": [
        "meta.diff.range",
        "meta.diff.index"
      ],
      "settings": {
        "foreground": "#B59F7B"
      }
    },
    {
      "name": "Diff Added Line",
      "scope": [
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#3F8B84"
      }
    },
    {
      "name": "Diff Removed Line",
      "scope": [
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#AD6A00"
      }
    },
    {
      "name": "Diff Changed Line",
      "scope": [
        "markup.changed.diff"
      ],
      "settings": {
        "foreground": "#B59F7B"
      }
    },
    {
      "name": "Diff Context Line",
      "scope": [
        "meta.diff.context",
        "meta.diff"
      ],
      "settings": {
        "foreground": "#586B55"
      }
    },
    {
      "name": "Diff Range Punctuation",
      "scope": [
        "punctuation.definition.range.diff"
      ],
      "settings": {
        "foreground": "#B59F7B"
      }
    },
    {
      "name": "Diff Meta Info",
      "scope": [
        "meta.diff.header",
        "meta.separator.diff"
      ],
      "settings": {
        "foreground": "#B59F7B"
      }
    },
    {
      "name": "Diff Inserted Punctuation",
      "scope": [
        "punctuation.definition.inserted.diff"
      ],
      "settings": {
        "foreground": "#3F8B84"
      }
    },
    {
      "name": "CSS",
      "scope": [
        "support.function.misc.css",
        "meta.function.variable.css"
      ],
      "settings": {
        "foreground": "#2F6F68"
      }
    }
  ]
}
