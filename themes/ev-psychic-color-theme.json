{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ev Psychic",
  "type": "light",
  "colors": {
    "foreground": "#7A5565",
    "editor.background": "#E9D8E3",
    "editor.foreground": "#4B3A45",
    "editorLineNumber.foreground": "#7C6472",
    "editorLineNumber.activeForeground": "#B63851",
    "editorCursor.foreground": "#5D5094",
