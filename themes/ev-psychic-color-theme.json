    "editor.lineHighlightBackground": "#F2E3EC",
    "editorWhitespace.foreground": "#B89CAF",
    "editorIndentGuide.background1": "#B89CAF",
    "editorIndentGuide.activeBackground1": "#7C6472",
    "activityBar.background": "#B695AA",
