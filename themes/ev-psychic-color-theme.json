    "activityBarBadge.background": "#B63851",
    "activityBarBadge.foreground": "#F6EFF3",
    "sideBar.background": "#E6CFDD",
    "sideBar.foreground": "#4B3A45",
    "sideBarSectionHeader.background": "#E9D8E3",
    "sideBarSectionHeader.foreground": "#4B3A45",
    "statusBar.background": "#B695AA",
    "statusBar.foreground": "#F6EFF3",
    "statusBar.debuggingBackground": "#7063A5",
    "statusBar.debuggingForeground": "#F6EFF3",
    "statusBar.noFolderBackground": "#E9D8E3",
    "statusBarItem.errorForeground": "#F6EFF3",
    "statusBarItem.warningForeground": "#F6EFF3",
    "statusBarItem.prominentForeground": "#F6EFF3",
    "titleBar.activeBackground": "#B695AA",
    "titleBar.activeForeground": "#F6EFF3",
    "titleBar.inactiveBackground": "#B695AA",
    "titleBar.inactiveForeground": "#F6EFF3",
    "tab.activeBackground": "#E9D8E3",
    "tab.activeForeground": "#302631",
    "tab.inactiveBackground": "#E9D8E3",
    "tab.inactiveForeground": "#9B8A95",
    "tab.hoverBackground": "#F2E3EC",
    "tab.border": "#B89CAF55",
    "tab.editor-group-tab-background": "#E6CFDD",
    "terminal.background": "#E9D8E3",
    "terminal.foreground": "#ffffff",
    "terminal.ansiBlack": "#D2BECB",
    "terminal.ansiRed": "#B63851",
    "terminal.ansiGreen": "#487AA9",
    "terminal.ansiYellow": "#EAD8E3",
    "terminalStickyScroll.background": "#D2BECB",
    "terminal.ansiBlue": "#5D5094",
    "terminal.ansiMagenta": "#C6728B",
    "terminal.ansiCyan": "#7063A5",
    "terminal.ansiWhite": "#7C6472",
    "terminal.ansiBrightBlack": "#A58E9D",
    "terminal.ansiBrightRed": "#C6728B",
    "terminal.ansiBrightGreen": "#6793BA",
    "terminal.ansiBrightYellow": "#F2E3EC",
    "terminal.ansiBrightBlue": "#7063A5",
    "terminal.ansiBrightMagenta": "#D6B9C7",
    "terminal.ansiBrightCyan": "#8CA4C3",
    "terminal.ansiBrightWhite": "#F6EFF3",
    "badge.background": "#B63851",
    "badge.foreground": "#F6EFF3",
    "button.background": "#5D5094",
    "button.hoverBackground": "#C9A8BA",
    "button.foreground": "#F6EFF3",
    "button.secondaryBackground": "#C9A8BA",
    "button.secondaryHoverBackground": "#D8C1CF",
    "button.secondaryForeground": "#4B3A45",
    "extensionButton.background": "#5D5094",
    "extensionButton.foreground": "#F6EFF3",
    "extensionButton.hoverBackground": "#7063A5",
    "extensionButton.prominentBackground": "#5D5094",
    "extensionButton.prominentForeground": "#F6EFF3",
    "extensionButton.prominentHoverBackground": "#7063A5",
    "editorWidget.foreground": "#4B3A45",
    "editorWidget.background": "#E9D8E3",
    "input.background": "#E9D8E3",
    "input.foreground": "#302631",
    "input.border": "#E9D8E3",
    "input.placeholderForeground": "#9B8A95",
    "dropdown.background": "#BFA3B5",
    "dropdown.foreground": "#4B3A45",
    "dropdown.border": "#B08DA3",
    "widget.shadow": "#7C647266",
    "scrollbar.shadow": "#7C647233",
    "panel.background": "#E9D8E3",
    "panel.border": "#B08DA3",
    "panelTitle.activeForeground": "#302631",
    "panelTitle.inactiveForeground": "#9B8A95",
    "scrollbarSlider.background": "#BFA3B566",
    "scrollbarSlider.hoverBackground": "#BFA3B577",
    "scrollbarSlider.activeBackground": "#BFA3B588",
    "list.activeSelectionBackground": "#DCC6D3",
    "list.activeSelectionForeground": "#4B3A45",
    "list.activeSelectionIconForeground": "#4B3A45",
    "list.hoverBackground": "#EAD8E3",
    "list.hoverForeground": "#4B3A45",
    "list.iconForeground": "#4B3A45",
    "list.inactiveSelectionIconForeground": "#4B3A45",
    "quickInputList.focusIconForeground": "#4B3A45",
    "list.inactiveSelectionBackground": "#D8C1CF",
    "list.focusBackground": "#DCC6D3",
    "list.highlightForeground": "#5D5094",
    "notifications.background": "#E9D8E3",
    "notifications.foreground": "#302631",
    "notifications.border": "#B08DA3",
    "notificationCenterHeader.background": "#B695AA",
    "notificationsInfoIcon.foreground": "#487AA9",
    "notificationsWarningIcon.foreground": "#B63851",
    "notificationsErrorIcon.foreground": "#B63851",
    "toolbar.hoverBackground": "#C9A8BA",
    "toolbar.hoverForeground": "#4B3A45",
    "toolbar.activeBackground": "#E6CFDD",
    "toolbar.activeForeground": "#4B3A45",
    "actionBar.toggledBackground": "#C9A8BA",
    "actionBar.toggledForeground": "#4B3A45",
    "menu.selectionBackground": "#C9A8BA",
    "menu.selectionForeground": "#4B3A45",
    "menubar.selectionBackground": "#C9A8BA",
    "menubar.selectionForeground": "#4B3A45",
    "icon.foreground": "#5D5094",
    "quickInput.background": "#E9D8E3",
    "quickInput.foreground": "#4B3A45",
    "quickInput.list.focusBackground": "#DCC6D3",
    "focusBorder": "#5D5094",
    "disabledForeground": "#7C6472",
    "descriptionForeground": "#9B8A95",
    "textLink.foreground": "#5D5094",
    "textLink.hoverForeground": "#5D5094",
    "editorStickyScroll.shadow": "#D2BECB",
    "statusBarItem.remoteForeground": "#F6EFF3",
    "statusBarItem.remoteBackground": "#5D5094",
    "tooltip.background": "#F0E3EA",
    "tooltip.foreground": "#4B3A45",
    "tooltip.border": "#F0E3EA",
    "editorHoverWidget.background": "#F0E3EA",
    "editorHoverWidget.foreground": "#4B3A45",
    "editorHoverWidget.border": "#F0E3EA",
    "editorHoverWidget.statusBarBackground": "#F0E3EA",
    "keybindingTable.rowsBackground": "#D2BECB",
    "panelSectionHeader.background": "#D2BECB",
    "editorGroupHeader.tabsBackground": "#E6CFDD",
    "editorGroupHeader.noTabsBackground": "#E6CFDD"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line",
        "comment.line.double-slash",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#9B8A95",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#5D5094"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#7063A5"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#7C6472"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": [
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#8B7A86"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#5D5094"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#B63851"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#B55C77"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#4A3540"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#4279A5"
      }
    },
    {
      "name": "Class & Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#3F6A93"
      }
    },
    {
      "name": "Annotation / Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#7063A5"
      }
    },
    {
      "name": "Operator / Punctuation",
      "scope": [
        "punctuation",
        "operator"
      ],
      "settings": {
        "foreground": "#8B7A86"
      }
    },
    {
      "name": "Invalid / Error",
      "scope": [
        "invalid",
        "invalid.deprecated",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#B63851"
      }
    },
    {
      "name": "Property / Object Key",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#5D5094"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#5D5094"
      }
    },
    {
      "name": "Special Variable",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#487AA9"
      }
    },
    {
      "name": "Module / Namespace",
      "scope": [
        "entity.name.type.module",
        "support.other.module"
      ],
      "settings": {
        "foreground": "#3F6A93"
      }
    },
    {
      "name": "text.html.derivative",
      "scope": [
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#4B3A45"
      }
    },
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "meta.tag.html",
        "meta.tag.xml",
        "meta.tag.other"
      ],
      "settings": {
        "foreground": "#5D5094"
      }
    },
    {
      "name": "HTML Attribute Name",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute",
        "meta.tag.xml.attribute"
      ],
      "settings": {
        "foreground": "#487AA9"
      }
    },
    {
      "name": "HTML Attribute Value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted.html"
      ],
      "settings": {
        "foreground": "#C6728B"
      }
    },
    {
      "name": "HTML Punctuation",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag",
        "punctuation.separator.key-value.html"
      ],
      "settings": {
        "foreground": "#8B7A86"
      }
    },
    {
      "name": "HTML Doctype / Comment",
      "scope": [
        "meta.tag.sgml.doctype",
        "comment.block.html"
      ],
      "settings": {
        "foreground": "#9B8A95",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX/TSX Tag",
      "scope": [
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#5D5094"
      }
    },
    {
      "name": "JSX Attribute",
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#487AA9"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "markup.heading.markdown",
        "markup.heading.setext",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#5D5094",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#B63851",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#B63851",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.title.markdown",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#487AA9",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Link URL",
      "scope": [
        "markup.underline.link",
        "markup.underline.link.image.markdown",
        "markup.underline.link.markdown",
        "markup.link.url.markdown"
      ],
      "settings": {
        "foreground": "#7063A5"
      }
    },
    {
      "name": "Markdown Code Inline",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.raw.inline.markdown"
      ],
      "settings": {
        "foreground": "#C6728B"
      }
    },
    {
      "name": "Markdown Code Block",
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.raw.block.markdown",
        "fenced_code.block.language",
        "fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#C6728B"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#DCC6D3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List / Bullet",
      "scope": [
        "markup.list",
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#7C6472"
      }
    },
    {
      "name": "Markdown Separator / HR",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "#9B8A95"
      }
    },
    {
      "name": "Markdown Comment (HTML-style)",
      "scope": [
        "comment.block.html.markdown"
      ],
      "settings": {
        "foreground": "#9B8A95",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Heading 1 (#)",
      "scope": [
        "punctuation.definition.heading.markdown",
        "heading.1.markdown",
        "markup.heading.markdown",
        "markup.heading.setext.markdown",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#5D5094",
        "fontStyle": "bold"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#7C6472"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#7C6472"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "meta.object-literal.key",
        "meta.object.member",
        "meta.objectliteral",
        "support.type.property-name",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#5D5094"
      }
    },
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "variable.other.property.js",
        "variable.other.object.property",
        "support.variable.property.dom",
        "meta.property.object.js"
      ],
      "settings": {
        "foreground": "#5D5094"
      }
    },
    {
      "name": "Method (dot chain)",
      "scope": [
        "support.function",
        "variable.function",
        "variable.function.js",
        "entity.name.function.member",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#4279A5"
      }
    },
    {
      "name": "Object variable (req, res, fs, helper, etc.)",
      "scope": [
        "variable.other.object.js",
        "variable.other.object.js.jsx"
      ],
      "settings": {
        "foreground": "#4A3540"
      }
    },
    {
      "name": "Promise",
      "scope": [
        "support.class.promise.js",
        "support.class.promise.ts"
      ],
      "settings": {
        "foreground": "#487AA9"
      }
    },
    {
      "name": "Diff Header (from-file)",
      "scope": [
        "meta.diff.header.from-file",
        "punctuation.definition.from-file.diff"
      ],
      "settings": {
        "foreground": "#B63851",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Header (to-file)",
      "scope": [
        "meta.diff.header.to-file",
        "punctuation.definition.to-file.diff"
      ],
      "settings": {
        "foreground": "#487AA9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Range / Index Line",
      "scope": [
        "meta.diff.range",
        "meta.diff.index"
      ],
      "settings": {
        "foreground": "#7063A5"
      }
    },
    {
      "name": "Diff Added Line",
      "scope": [
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#487AA9"
      }
    },
    {
      "name": "Diff Removed Line",
      "scope": [
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#B63851"
      }
    },
    {
      "name": "Diff Changed Line",
      "scope": [
        "markup.changed.diff"
      ],
      "settings": {
        "foreground": "#C6728B"
      }
    },
    {
      "name": "Diff Context Line",
      "scope": [
        "meta.diff.context",
        "meta.diff"
      ],
      "settings": {
        "foreground": "#7C6472"
      }
    },
    {
      "name": "Diff Range Punctuation",
      "scope": [
        "punctuation.definition.range.diff"
      ],
      "settings": {
        "foreground": "#7063A5"
      }
    },
    {
      "name": "Diff Meta Info",
      "scope": [
        "meta.diff.header",
        "meta.separator.diff"
      ],
      "settings": {
        "foreground": "#5D5094"
      }
    },
    {
      "name": "Diff Inserted Punctuation",
      "scope": [
        "punctuation.definition.inserted.diff"
      ],
      "settings": {
        "foreground": "#487AA9"
      }
    },
    {
      "name": "CSS",
      "scope": [
        "support.function.misc.css",
        "meta.function.variable.css"
      ],
      "settings": {
        "foreground": "#4279A5"
      }
    }
  ]
}
