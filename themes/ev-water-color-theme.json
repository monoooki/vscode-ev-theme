    "activityBar.background": "#00647D",
    "activityBar.foreground": "#E8EFFA",
    "activityBarBadge.background": "#F47774",
    "activityBarBadge.foreground": "#E8EFFA",
    "sideBar.background": "#C8D2DB",
    "sideBar.foreground": "#1F252B",
    "sideBarSectionHeader.background": "#D8DDDF",
    "sideBarSectionHeader.foreground": "#1F252B",
    "sideBarSectionHeader.border": "#C8D2DB",
    "statusBar.background": "#00647D",
    "statusBar.foreground": "#E8EFFA",
    "statusBar.debuggingBackground": "#21859A",
    "statusBar.debuggingForeground": "#E8EFFA",
    "statusBar.noFolderBackground": "#E8EFFA",
    "titleBar.activeBackground": "#00647D",
    "titleBar.activeForeground": "#E8EFFA",
    "titleBar.inactiveBackground": "#00647D",
    "titleBar.inactiveForeground": "#E8EFFA",
    "commandCenter.foreground": "#E8EFFA",
    "commandCenter.activeForeground": "#E8EFFA",
    "commandCenter.inactiveForeground": "#E8EFFA",
    "tab.activeBackground": "#E8EFFA",
    "tab.activeForeground": "#00647D",
    "tab.inactiveBackground": "#C8D2DB",
    "tab.inactiveForeground": "#557076",
    "tab.hoverBackground": "#E8EFFA",
    "tab.editor-group-tab-background": "#C8D2DB",
    "tab.border": "#C8D2DB",
    "tab.activeBorder": "#00000000",
    "tab.activeBorderTop": "#00000000",
    "tab.unfocusedActiveBorder": "#00000000",
    "tab.unfocusedActiveBorderTop": "#00000000",
    "editorGroupHeader.tabsBorder": "#C8D2DB",
    "terminal.background": "#E8EFFA",
    "terminal.foreground": "#1F252B",
    "terminal.ansiBlack": "#D8DDDF",
    "terminal.ansiRed": "#F47774",
    "terminal.ansiGreen": "#54B2C5",
    "terminal.ansiYellow": "#DBD6AA",
    "terminalStickyScroll.background": "#D8DDDF",
    "terminal.ansiBlue": "#21859A",
    "terminal.ansiMagenta": "#665A5E",
    "terminal.ansiCyan": "#70D1E8",
    "terminal.ansiWhite": "#7B8282",
    "terminal.ansiBrightBlack": "#A7A398",
    "terminal.ansiBrightRed": "#F47774",
    "terminal.ansiBrightGreen": "#70D1E8",
    "terminal.ansiBrightYellow": "#F2F0C1",
    "terminal.ansiBrightBlue": "#54B2C5",
    "terminal.ansiBrightMagenta": "#BCBBBE",
    "terminal.ansiBrightCyan": "#70D1E8",
    "terminal.ansiBrightWhite": "#E8EFFA",
    "badge.background": "#F47774",
    "badge.foreground": "#E8EFFA",
    "button.background": "#00647D",
    "button.hoverBackground": "rgba(84, 178, 197, 0.2)",
    "button.foreground": "#E8EFFA",
    "button.secondaryBackground": "#E8EFFA",
    "button.secondaryHoverBackground": "#D8DDDF",
    "button.secondaryForeground": "#1F252B",
    "extensionButton.background": "#00647D",
    "extensionButton.foreground": "#E8EFFA",
    "extensionButton.hoverBackground": "#21859A",
    "extensionButton.prominentBackground": "#54B2C5",
    "extensionButton.prominentForeground": "#E8EFFA",
    "extensionButton.prominentHoverBackground": "#70D1E8",
    "editorWidget.foreground": "#1F252B",
    "editorWidget.background": "#E8EFFA",
    "input.background": "#E8EFFA",
    "input.foreground": "#21859A",
    "input.border": "#E8EFFA",
    "input.placeholderForeground": "#A7A398",
    "dropdown.background": "#C8D2DB",
    "dropdown.foreground": "#1F252B",
    "dropdown.border": "#C8D2DB",
    "widget.shadow": "#7B828266",
    "scrollbar.shadow": "#7B828233",
    "panel.background": "#E8EFFA",
    "panel.border": "#C8D2DB",
    "panelTitle.activeForeground": "#21859A",
    "panelTitle.inactiveForeground": "#557076",
    "scrollbarSlider.background": "#C8D2DB66",
    "scrollbarSlider.hoverBackground": "#C8D2DB77",
    "scrollbarSlider.activeBackground": "#C8D2DB88",
    "list.activeSelectionBackground": "#D8DDDF",
    "list.activeSelectionForeground": "#1F252B",
    "list.activeSelectionIconForeground": "#1F252B",
    "list.hoverBackground": "#D8DDDF",
    "list.hoverForeground": "#1F252B",
    "list.iconForeground": "#1F252B",
    "list.inactiveSelectionIconForeground": "#1F252B",
    "quickInputList.focusIconForeground": "#1F252B",
    "toolbar.hoverBackground": "#54B2C533",
    "toolbar.hoverForeground": "#1F252B",
    "toolbar.activeBackground": "#C8D2DB",
    "toolbar.activeForeground": "#1F252B",
    "actionBar.toggledBackground": "rgba(84, 178, 197, 0.2)",
    "menu.selectionBackground": "rgba(84, 178, 197, 0.2)",
    "menubar.selectionBackground": "rgba(84, 178, 197, 0.2)",
    "list.inactiveSelectionBackground": "#E8EFFA",
    "list.focusBackground": "#D8DDDF",
    "list.highlightForeground": "#00647D",
    "notifications.background": "#E8EFFA",
    "notifications.foreground": "#21859A",
    "notifications.border": "#C8D2DB",
    "notificationCenterHeader.background": "#BCBBBE",
    "notificationsInfoIcon.foreground": "#21859A",
    "notificationsWarningIcon.foreground": "#DBD6AA",
    "notificationsErrorIcon.foreground": "#F47774",
    "icon.foreground": "#00647D",
    "quickInput.background": "#E8EFFA",
    "quickInput.foreground": "#1F252B",
    "quickInput.list.focusBackground": "#D8DDDF",
    "focusBorder": "#54B2C5",
    "disabledForeground": "#7B8282",
    "descriptionForeground": "#557076",
    "textLink.foreground": "#00647D",
    "textLink.hoverForeground": "#00647D",
    "editorStickyScroll.shadow": "#D8DDDF",
    "statusBarItem.remoteForeground": "#E8EFFA",
    "statusBarItem.remoteBackground": "#00647D",
    "tooltip.background": "#D8DDDF",
    "tooltip.foreground": "#1F252B",
    "tooltip.border": "#C8D2DB",
    "keybindingTable.rowsBackground": "#D8DDDF",
    "panelSectionHeader.background": "#D8DDDF",
    "editorGroupHeader.tabsBackground": "#E8EFFA",
    "editorGroupHeader.noTabsBackground": "#E8EFFA"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line",
        "comment.line.double-slash",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#857E75",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#00647D"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#21859A"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#7B8282"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": [
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#838186"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#00647D"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#D4574F"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#2786A9"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#1F252B"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#3B8BB3"
      }
    },
    {
      "name": "Class & Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#8E7D30"
      }
    },
    {
      "name": "Annotation / Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#54B2C5"
      }
    },
    {
      "name": "Operator / Punctuation",
      "scope": [
        "punctuation",
        "operator"
      ],
      "settings": {
        "foreground": "#838186"
      }
    },
    {
      "name": "Invalid / Error",
      "scope": [
        "invalid",
        "invalid.deprecated",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#D4574F"
      }
    },
    {
      "name": "Property / Object Key",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#003C53"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#00647D"
      }
    },
    {
      "name": "Special Variable",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#665A5E"
      }
    },
    {
      "name": "Module / Namespace",
      "scope": [
        "entity.name.type.module",
        "support.other.module"
      ],
      "settings": {
        "foreground": "#00647D"
      }
    },
    {
      "name": "text.html.derivative",
      "scope": [
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#1F252B"
      }
    },
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "meta.tag.html",
        "meta.tag.xml",
        "meta.tag.other"
      ],
      "settings": {
        "foreground": "#00647D"
      }
    },
    {
      "name": "HTML Attribute Name",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute",
        "meta.tag.xml.attribute"
      ],
      "settings": {
        "foreground": "#3B8BB3"
      }
    },
    {
      "name": "HTML Attribute Value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted.html"
      ],
      "settings": {
        "foreground": "#2786A9"
      }
    },
    {
      "name": "HTML Punctuation",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag",
        "punctuation.separator.key-value.html"
      ],
      "settings": {
        "foreground": "#7B8282"
      }
    },
    {
      "name": "HTML Doctype / Comment",
      "scope": [
        "meta.tag.sgml.doctype",
        "comment.block.html"
      ],
      "settings": {
        "foreground": "#857E75",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX/TSX Tag",
      "scope": [
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#00647D"
      }
    },
    {
      "name": "JSX Attribute",
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#3B8BB3"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "markup.heading.markdown",
        "markup.heading.setext",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#00647D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#D4574F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#D4574F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.title.markdown",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#00647D",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Link URL",
      "scope": [
        "markup.underline.link",
        "markup.underline.link.image.markdown",
        "markup.underline.link.markdown",
        "markup.link.url.markdown"
      ],
      "settings": {
        "foreground": "#21859A"
      }
    },
    {
      "name": "Markdown Code Inline",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.raw.inline.markdown"
      ],
      "settings": {
        "foreground": "#21859A"
      }
    },
    {
      "name": "Markdown Code Block",
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.raw.block.markdown",
        "fenced_code.block.language",
        "fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#21859A"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#8A7F3D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List / Bullet",
      "scope": [
        "markup.list",
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#7B8282"
      }
    },
    {
      "name": "Markdown Separator / HR",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "#838186"
      }
    },
    {
      "name": "Markdown Comment (HTML-style)",
      "scope": [
        "comment.block.html.markdown"
      ],
      "settings": {
        "foreground": "#857E75",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Heading 1 (#)",
      "scope": [
        "punctuation.definition.heading.markdown",
        "heading.1.markdown",
        "markup.heading.markdown",
        "markup.heading.setext.markdown",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#00647D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#7B8282"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#7B8282"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "meta.object-literal.key",
        "meta.object.member",
        "meta.objectliteral",
        "support.type.property-name",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#003C53"
      }
    },
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "variable.other.property.js",
        "variable.other.object.property",
        "support.variable.property.dom",
        "meta.property.object.js"
      ],
      "settings": {
        "foreground": "#003C53"
      }
    },
    {
      "name": "Method (dot chain)",
      "scope": [
        "support.function",
        "variable.function",
        "variable.function.js",
        "entity.name.function.member",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#21859A"
      }
    },
    {
      "name": "Object variable (req, res, fs, helper, etc.)",
      "scope": [
        "variable.other.object.js",
        "variable.other.object.js.jsx"
      ],
      "settings": {
        "foreground": "#1F252B"
      }
    },
    {
      "name": "Promise",
      "scope": [
        "support.class.promise.js",
        "support.class.promise.ts"
      ],
      "settings": {
        "foreground": "#665A5E"
      }
    },
    {
      "name": "Diff Header (from-file)",
      "scope": [
        "meta.diff.header.from-file",
        "punctuation.definition.from-file.diff"
      ],
      "settings": {
        "foreground": "#D4574F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Header (to-file)",
      "scope": [
        "meta.diff.header.to-file",
        "punctuation.definition.to-file.diff"
      ],
      "settings": {
        "foreground": "#3B8BB3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Range / Index Line",
      "scope": [
        "meta.diff.range",
        "meta.diff.index"
      ],
      "settings": {
        "foreground": "#948634"
      }
    },
    {
      "name": "Diff Added Line",
      "scope": [
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#3B8BB3"
      }
    },
    {
      "name": "Diff Removed Line",
      "scope": [
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#D4574F"
      }
    },
    {
      "name": "Diff Changed Line",
      "scope": [
        "markup.changed.diff"
      ],
      "settings": {
        "foreground": "#8D8035"
      }
    },
    {
      "name": "Diff Context Line",
      "scope": [
        "meta.diff.context",
        "meta.diff"
      ],
      "settings": {
        "foreground": "#7B8282"
      }
    },
    {
      "name": "Diff Range Punctuation",
      "scope": [
        "punctuation.definition.range.diff"
      ],
      "settings": {
        "foreground": "#948634"
      }
    },
    {
      "name": "Diff Meta Info",
      "scope": [
        "meta.diff.header",
        "meta.separator.diff"
      ],
      "settings": {
        "foreground": "#8D8035"
      }
    },
    {
      "name": "Diff Inserted Punctuation",
      "scope": [
        "punctuation.definition.inserted.diff"
      ],
      "settings": {
        "foreground": "#2786A9"
      }
    },
    {
      "name": "CSS",
      "scope": [
        "support.function.misc.css",
        "meta.function.variable.css"
      ],
      "settings": {
        "foreground": "#3B8BB3"
      }
    }
  ]
}
